{
  "name": "webapp-team-example",
  "version": "1.0.0",
  "description": "Example webapp for compliance testing",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "start:with-migrations": "./start.sh",
    "test": "jest --coverage",
    "test:ci": "jest --ci --coverage --coverageReporters=text-summary",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write '**/*.{js,json,md}'",
    "format:check": "prettier --check '**/*.{js,json,md}'",
    "migrate": "node migrate.js",
    "migrate:up": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "migrate:create": "node-pg-migrate create"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^6.1.0",
    "express": "^4.18.2",
    "node-pg-migrate": "^8.0.3",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  },
  "author": "webapp-team@u2i.com",
  "license": "UNLICENSED",
  "compliance": {
    "iso27001": true,
    "soc2-type2": true,
    "gdpr": true,
    "data-residency": "eu"
  }
}
